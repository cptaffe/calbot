BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//connor.zip//CalBot v1.0//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
NAME:Little Rock Soirée Weekend Guide
X-WR-CALNAME:Little Rock Soirée Weekend Guide
DESCRIPTION:Autogenerated iCal pulled from the Little Rock Soirée Weekend Guide website
X-WR-CALDESC:Autogenerated iCal pulled from the Little Rock Soirée Weekend Guide website
URL:https://connor.zip/calbot/soiree
REFRESH-INTERVAL;VALUE=DURATION:PT1H
{{- range . -}}
{{- $event := . }}
{{- if .Title -}}
{{- if .Times -}}
{{- range .Times }}
BEGIN:VEVENT
DTSTART;VALUE=DATE-TIME;TZID=America/Chicago:{{ time .Start }}
{{- if not .End.IsZero }}
DTEND;VALUE=DATE-TIME;TZID=America/Chicago:{{ time .End }}
{{- end }}
DTSTAMP:{{ time now }}
UID:{{ uuid }}
CREATED:{{ time now }}
DESCRIPTION:{{ escape $event.Body.String | wrap }}
{{- if $event.Link }}
URL:{{ $event.Link }}
{{- end }}
LAST-MODIFIED:{{ time now }}
LOCATION:
SEQUENCE:0
STATUS:CONFIRMED
SUMMARY:{{ $event.Title }}
TRANSP:OPAQUE
END:VEVENT
{{- end -}}
{{- else }}
BEGIN:VEVENT
DTSTART;VALUE=DATE;TZID=America/Chicago:{{ date .Dates.Start }}
{{- if not .Dates.End.IsZero }}
DTEND;VALUE=DATE;TZID=America/Chicago:{{ date .Dates.End }}
{{- end }}
DTSTAMP:{{ time now }}
UID:{{ uuid }}
CREATED:{{ time now }}
DESCRIPTION:{{ escape .Body.String | wrap }}
{{- if .Link }}
URL:{{ .Link }}
{{- end }}
LAST-MODIFIED:{{ time now }}
LOCATION:
SEQUENCE:0
STATUS:CONFIRMED
SUMMARY:{{.Title}}
TRANSP:OPAQUE
END:VEVENT
{{- end -}}
{{- end -}}
{{- end }}
END:VCALENDAR
